		return 0
	}
	if {$lamer == ""} {
		putserv "PRIVMSG $chan :4Modo De Uso: !PiRoPo <nick>"
	} else {
		putserv "PRIVMSG $chan :Un PiRoPo PaRa $lamer: $word"
