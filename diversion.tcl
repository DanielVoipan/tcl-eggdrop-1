proc ircadicto {nick host hand chan arg} {
	global ircadicto
	set lamer [lindex $arg 0]
	set word [lindex $ircadicto [rand [llength $ircadicto]]]
	if {$lamer == ""} {
		putserv "PRIVMSG $chan :4Modo De Uso: !AdIcToIrC <nick>"
	} else {
		putserv "PRIVMSG $chan :\002$lamer\002: ErEs Un IrCaDicTo $word"
	}

}

